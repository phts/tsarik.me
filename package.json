{
  "name": "tsaryk.com",
  "private": true,
  "scripts": {
    "build": "npm run build:prod --",
    "build:dev": "webpack",
    "build:prod": "webpack --env.prod",
    "build:prod:relative": "npm run build:prod -- --env.relative",
    "clean": "rimraf ./dist",
    "deploy:dev": "node ./scripts/deploy-dev.js",
    "deploy:prod": "node ./scripts/deploy-prod.js",
    "eslint": "eslint . --ext .js,.ts,.tsx",
    "fix": "npm run eslint -- --fix",
    "lint": "run-p typelint eslint",
    "prebuild:dev": "npm run clean",
    "prebuild:prod": "npm run clean",
    "serve": "ws --directory ./dist --spa ./index.html --port 8080",
    "start": "npm run start:dev",
    "start:dev": "webpack-dev-server",
    "start:dev:external": "npm run start:dev -- --host 0.0.0.0",
    "start:prod": "npm run build && npm run serve",
    "test": "jest",
    "typelint": "tsc --noEmit"
  },
  "engines": {
    "node": ">= 7.6"
  },
  "dependencies": {
    "browser-locale": "^1.0.3",
    "date-fns": "^1.30.1",
    "emailjs-com": "^2.3.2",
    "mobx": "^5.13.0",
    "mobx-react": "^6.1.3",
    "rambdax": "^2.10.0",
    "ramda": "^0.26.1",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-router-dom": "^5.0.1",
    "styled-components": "^4.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/plugin-transform-object-assign": "^7.2.0",
    "@babel/preset-env": "^7.4.3",
    "@phts/eslint-config": "^1.6.2",
    "@phts/prettier-config": "^1.0.4",
    "@types/history": "^4.7.2",
    "@types/jest": "^24.0.11",
    "@types/ramda": "^0.26.6",
    "@types/react": "^16.9.1",
    "@types/react-dom": "^16.8.5",
    "@types/react-router-dom": "^4.3.4",
    "@types/styled-components": "4.1.8",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-date-fns": "^0.2.1",
    "babel-plugin-ramda": "^2.0.0",
    "eslint": "^6.1.0",
    "favicons-webpack-plugin": "1.0.1",
    "file-loader": "^4.0.0",
    "gh-pages": "^2.0.1",
    "html-webpack-plugin": "^3.2.0",
    "image-webpack-loader": "^5.0.0",
    "jest": "^24.7.1",
    "jimp": "^0.6.1",
    "local-web-server": "^3.0.0",
    "md5-file": "^4.0.0",
    "mockdate": "^2.0.5",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.18.2",
    "responsive-loader": "^1.2.0",
    "rimraf": "^3.0.0",
    "shell-escape": "^0.2.0",
    "svg-react-loader": "^0.4.6",
    "svg-sprite-loader": "^4.1.3",
    "ts-jest": "^24.0.2",
    "typescript": "^3.5.3",
    "uglifyjs-webpack-plugin": "^2.1.2",
    "webpack": "^4.38.0",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.3.1",
    "webpack-merge": "^4.2.1",
    "webpack-replace-loader": "^1.3.0"
  },
  "jest": {
    "transform": {
      "^.+rambdax.cjs.js": "<rootDir>/config/jest-empty-preprocessor.js",
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "\\.test\\.tsx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ],
    "moduleDirectories": [
      "app",
      "node_modules"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(rambdax)/)"
    ]
  }
}
