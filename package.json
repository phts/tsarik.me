{
  "name": "tsaryk.com",
  "private": true,
  "scripts": {
    "build": "npm run build:prod --",
    "build:dev": "npm run webpack",
    "build:prod": "npm run webpack -- --env.prod",
    "build:prod:relative": "npm run build:prod -- --env.relative",
    "clean": "rimraf ./dist",
    "deploy:dev": "node ./scripts/deploy-dev.js",
    "deploy:prod": "node ./scripts/deploy-prod.js",
    "eslint": "eslint . --ext .js,.ts,.tsx",
    "fix": "npm run eslint -- --fix",
    "format": "prettier --write '**/*.{js,ts,tsx,html,md,json}'",
    "lint": "run-p typelint eslint",
    "prebuild:dev": "npm run clean",
    "prebuild:prod": "npm run clean",
    "serve": "ws --directory ./dist --spa ./index.html --port 8080",
    "start": "npm run start:dev",
    "start:dev": "npm run webpack-dev-server",
    "start:dev:external": "npm run start:dev -- --host 0.0.0.0",
    "start:prod": "npm run build && npm run serve",
    "test": "jest",
    "typelint": "tsc --noEmit",
    "webpack": "TS_NODE_PROJECT=config/webpack.tsconfig.json webpack",
    "webpack-dev-server": "TS_NODE_PROJECT=config/webpack.tsconfig.json webpack-dev-server"
  },
  "engines": {
    "node": ">= 7.6"
  },
  "dependencies": {
    "browser-locale": "^1.0.3",
    "date-fns": "^2.0.0",
    "emailjs-com": "^2.3.2",
    "mobx": "^5.13.0",
    "mobx-react": "^6.1.3",
    "rambdax": "^2.13.0",
    "ramda": "^0.26.1",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-router-dom": "^5.0.1",
    "styled-components": "^4.3.2"
  },
  "devDependencies": {
    "@phts/eslint-config": "^1.8.1",
    "@phts/prettier-config": "^1.0.4",
    "@types/history": "^4.7.3",
    "@types/html-webpack-plugin": "^3.2.1",
    "@types/jest": "^24.0.18",
    "@types/md5-file": "^4.0.0",
    "@types/node": "^12.7.2",
    "@types/ramda": "^0.26.19",
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.9.0",
    "@types/react-router-dom": "^4.3.5",
    "@types/styled-components": "4.1.8",
    "@types/uglifyjs-webpack-plugin": "^1.1.0",
    "@types/webpack": "^4.39.0",
    "@types/webpack-dev-server": "^3.1.7",
    "@types/webpack-merge": "^4.1.5",
    "awesome-typescript-loader": "^5.2.1",
    "eslint": "^6.2.1",
    "favicons-webpack-plugin": "1.0.1",
    "file-loader": "^4.2.0",
    "gh-pages": "^2.1.1",
    "html-webpack-plugin": "^3.2.0",
    "image-webpack-loader": "^5.0.0",
    "jest": "^24.9.0",
    "jimp": "^0.6.4",
    "local-web-server": "^3.0.7",
    "md5-file": "^4.0.0",
    "mockdate": "^2.0.5",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.18.2",
    "responsive-loader": "^1.2.0",
    "rimraf": "^3.0.0",
    "shell-escape": "^0.2.0",
    "svg-react-loader": "^0.4.6",
    "svg-sprite-loader": "^4.1.6",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.3.0",
    "tsconfig-paths": "^3.8.0",
    "typescript": "^3.5.3",
    "uglifyjs-webpack-plugin": "^2.1.2",
    "webpack": "^4.39.2",
    "webpack-bundle-analyzer": "^3.4.1",
    "webpack-cli": "^3.3.7",
    "webpack-dev-server": "^3.8.0",
    "webpack-merge": "^4.2.1",
    "webpack-replace-loader": "^1.3.0"
  },
  "jest": {
    "transform": {
      "^.+rambdax.cjs.js": "<rootDir>/config/jest-empty-preprocessor.js",
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "\\.test\\.tsx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ],
    "moduleDirectories": [
      "app",
      "node_modules"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(rambdax)/)"
    ]
  }
}
